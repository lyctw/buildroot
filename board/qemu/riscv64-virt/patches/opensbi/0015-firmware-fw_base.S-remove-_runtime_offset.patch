From 9c8b18eb01b80f4a29d9c7be463b645b41fc6664 Mon Sep 17 00:00:00 2001
From: Xiang W <wxjstz@126.com>
Date: Wed, 17 Jan 2024 18:41:59 +0800
Subject: [PATCH 15/63] firmware: fw_base.S: remove _runtime_offset

_runtime_offset is a variable not used elsewhere, so remove it.

Signed-off-by: Xiang W <wxjstz@126.com>
Reviewed-by: Anup Patel <anup@brainfault.org>
---
 firmware/fw_base.S | 6 ------
 1 file changed, 6 deletions(-)

diff --git a/firmware/fw_base.S b/firmware/fw_base.S
index 3425fe5..09894e6 100644
--- a/firmware/fw_base.S
+++ b/firmware/fw_base.S
@@ -74,8 +74,6 @@ _try_lottery:
 	REG_L	t0, 0(t0)
 	/* t1 shall has the address of _fw_start */
 	sub	t2, t1, t0
-	lla	t3, _runtime_offset
-	REG_S	t2, (t3)
 	lla	t0, __rel_dyn_start
 	lla	t1, __rel_dyn_end
 	beq	t0, t1, _relocate_done
@@ -500,10 +498,6 @@ _skip_trap_exit_rv32_hyp:
 
 	.data
 	.align 3
-#ifdef FW_PIC
-_runtime_offset:
-	RISCV_PTR	0
-#endif
 _relocate_lottery:
 	RISCV_PTR	0
 _boot_status:
-- 
2.34.1

